{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "requirements": "requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL": "1"
  },
  "functions": {
    "app/main.py": {
      "maxDuration": 60,
      "memory": 2048
    }
  },
  "regions": ["gru1"]
} 